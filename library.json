{
    "name": "usbpd-stack",
    "version": "0.1.0",
    "description": "MCUXpresso USB Power Delivery stack with PTN5110 driver and ports for ESP32-S3 and Arduino.",
    "keywords": [
        "usb",
        "usb-c",
        "power-delivery",
        "ptn5110"
    ],
    "license": "BSD-3-Clause",
    "frameworks": [
        "arduino",
        "espidf"
    ],
    "platforms": "*",
    "build": {
        "srcDir": "pd",
        "includeDir": "pd",
        "srcFilter": [
            "+<*>",
            "-<compliance_test_report/>"
        ]
    }
}